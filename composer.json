{
    "name": "magento24/geotools",
    "type": "library",
    "description": "Geo-related tools PHP 7.0+ library",
    "keywords": ["geotools", "geometry", "geocoder", "geocoding", "geoip", "bounds", "distance", "batch", "async", "geohash", "geolocation", "latlong", "latitude", "longitude"],
    "license": "MIT",

    "authors": [{
        "name": "Muthupandi",
        "email": "muthupandi845@gmail.com",
        "homepage": "https://github.com/muthupandi",
        "role": "Developer"
    }],

    "support": {
        "issues": "https://github.com/muthupandi",
        "source": "https://github.com/muthupandi"
    },

    "replace": {
        "toin0u/geotools": "*"
    },

    "require": {
        "php": "^7.0",
        "psr/cache": "^1.0",
        "willdurand/geocoder": "^4.2",
        "react/promise": "^2.2",
        "symfony/console": "^2.7 || ^3.0 || ^4.0 || ^5.0",
        "symfony/property-access": "^2.7 || ^3.0 || ^4.0 || ^5.0",
        "symfony/serializer": "^2.7 || ^3.0 || ^4.0 || ^5.0",
        "react/event-loop": "0.4.*",
        "php-http/discovery": "^1.0",
        "ext-bcmath": "*"
    },

    "require-dev": {
        "phpunit/phpunit": "^6.5.5 || ^7.0"
    },

    "autoload": {
        "psr-4": {
            "Magento24\\Geotools\\": "src",
            "Magento24\\Geotools\\Tests\\": "tests"
        }
    },

    "bin": ["bin/geotools"],

    "extra": {
        "branch-alias": {
            "dev-master": "0.7-dev"
        }
    }
}